{
  "manifest_version": 2,

  "name": "Fb emoji",
  "description": "Slack styled emojis on your facebook chat",
  "version": "0.0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "icons":{
      "64" : "icon.png"
  },

  "web_accessible_resources": [
    "emojis.json"
  ],

  "content_scripts": [
      {
         "matches": ["https://www.facebook.com/*"],
         "js": [ "jquery-2.1.4.min.js", "test.js"]
      }
    ]
}
